{
  "name": "globetrotter",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "build": "ng build",
    "deploy-dev": "npm run build && cd dist/globetrotter && mv index.html 200.html && surge . --domain https://globetrotter.surge.sh",
