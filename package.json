    "@angular/forms": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.2",
    "@auth0/angular-jwt": "^5.0.2",
    "@boninger-works/state": "^3.2.0",
    "core-js": "^3.8.2",
    "husky": "^4.3.8",
    "lodash-es": "^4.17.20",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
